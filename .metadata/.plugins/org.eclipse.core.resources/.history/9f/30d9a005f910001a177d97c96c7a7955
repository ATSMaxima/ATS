package TEST;

import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

import Generic.ExcelReader;

public class Test_DataProviderATSRegistrarion extends BaseTest{
	@Test(dataProvider="Authentication")
	public void DD_Registration_data(String sUserName,String sPassword)throws  Exception
	{
		l = extent.createTest("DD_Registration_data");
		
		//CommonActions.homePageVerify(driver, 10, "Applicant Tracking System");
		for (int i = 1; i < 5; i++) 
		{	
			//String str= driver.getTitle();
			if(driver.getTitle().contains("Register"))
			{
				ExcelReader.setExcelData("Sheet1", i, 6, "fail");
			}
			else if (driver.getTitle().contains("Applicant Tracking System"))
			{	
				ExcelReader.setExcelData("Sheet1", i, 6, "pass");						
			}
		}
	}
	
	@DataProvider
	public Object[][] Authentication() throws Exception
	{
		Object[][] testObjArray = ExcelReader.getTableArray(DD_Registration_EXCEL,SHEET_NAME);
		
		System.out.println("array is " +testObjArray.toString());
		return testObjArray;
	}

}
