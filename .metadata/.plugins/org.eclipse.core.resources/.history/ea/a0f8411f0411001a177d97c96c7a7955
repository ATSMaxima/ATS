	public static Object[][] getTableArray() throws Exception {
		String[][] tabObjArray = null;
		try {
			FileInputStream ExcelFile = new FileInputStream("../ATS_HR/ExcelFiles/ATS_Registration_1.xlsx");
			ExcelWBook = WorkbookFactory.create(ExcelFile);
			ExcelWSheet = ExcelWBook.getSheet(0);
			int ci, cj;
			int totalRows = ExcelWSheet.getLastRowNum();
			int cellNo = ExcelWSheet.getRow(0).getLastCellNum();
			System.out.println(totalRows);
			// you can write a function as well to get Column count
			int totalCols = cellNo;
			tabObjArray = new String[totalRows][totalCols];
			ci = 0;
			for (int i = 1; i <= totalRows; i++, ci++) {
				cj = 0;
				//int cellNo = ExcelWSheet.getRow(i).getLastCellNum();
				for (int j = 0; j <= cellNo; j++, cj++) {
					//if (j == 0) {
						tabObjArray[ci][cj] = getCellData(i, j);
						System.out.println(tabObjArray[ci][cj]);
					//}
//					if (j == 1) {
//						tabObjArray[ci][cj] = getCellData(i, j);
//						//System.out.println(tabObjArray[ci][cj]);
//					}
				}
			}
			
		} catch (Exception e) {
			System.out.println(e);
		}
		return (tabObjArray);
	}
	

}